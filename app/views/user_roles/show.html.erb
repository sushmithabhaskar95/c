<table class="table table-striped">
<tr>
<td>Id</td>
<td>Name</td>
<td>Role</td>
<td>Manage</td>
</tr>
<%= @user_role.each do |f| %>
<tr>
<td> <%= f.id %> </td>
<td> <%= f.user&first_name %> </td>
<td> <%= f.role&.name %> </td>
<td> <%= link_to 'Destroy',f,method: :delete,data:{confirm:'Are you sure'} %> </td>
</tr>
<%end%>
</table>
<%= link_to t('.back', :default => t("helpers.links.back")),
users_path, :class => 'btn btn-default' %>